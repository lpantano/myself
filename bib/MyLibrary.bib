journal = {RNA (New York, N.Y.)},
keywords = {Humans,Long Interspersed Nucleotide Elements,Male,MicroRNAs,MicroRNAs: genetics,Phylogeny,Promoter Regions, Genetic,Proteins,Proteins: metabolism,Pseudogenes,RNA, Small Interfering,RNA, Small Interfering: genetics,RNA, Small Interfering: metabolism,Sequence Analysis, RNA,Sequence Analysis, RNA: methods,Spermatozoa,Spermatozoa: metabolism,Testis,Testis: metabolism},
month = {jun},
